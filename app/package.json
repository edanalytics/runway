{
  "name": "@edanalytics/runway",
  "version": "0.0.0",
  "scripts": {
    "fe:build": "nx run fe:build",
    "fe:typecheck": "nx run fe:typecheck",
    "api:build": "nx run api:build",
    "api:typecheck": "nx run api:typecheck",
    "api:start": "nx run api:serve",
    "api:start-deployed": "node dist/api/main.js",
    "api:generate-erd-local": "docker compose --env-file ./api/.env -f ./api/schemaspy/docker-compose.local.yml up",
    "api:generate-and-view-erd-local": "npm run api:generate-erd-local && serve ./api/schemaspy/output",
    "common-ui:storybook": "nx run common-ui:storybook",
    "api:test": "nx run api:test --verbose",
    "utils:test": "nx run utils:test",
    "api:migrate-local-dev": "node api/src/database/postgrator/migrate-standalone.mjs",
    "prisma:map-sql-js-cases": "prisma-case-format --file ./api/src/database/prisma/schema.prisma --map-field-case snake",
    "prisma:generate-client": "prisma generate --schema=./api/src/database/prisma/schema.prisma",
    "prisma:db-pull": "prisma db pull --schema=./api/src/database/prisma/schema.prisma",
    "prisma:pull-and-generate": "npm run prisma:db-pull && npm run prisma:map-sql-js-cases && npm run prisma:generate-client",
    "generate-entity": "node generate.js",
    "lint": "nx run-many --target lint --projects api fe models utils",
    "prettier:check": "prettier --check .",
    "prettier": "prettier --write .",
    "sandworm": "sandworm audit"
  },
